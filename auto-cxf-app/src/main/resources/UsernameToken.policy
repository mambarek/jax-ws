<wsp:Policy
		xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
		xmlns:sp="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702"
		xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"		
		wsu:Id="TestSecurityPolicy">
<!--
	xmlns:sp="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702"

	xmlns:sp="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy"
	xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/08/addressing"
-->

	<wsp:ExactlyOne>
			<wsp:All>

				<!--Definition des Transportbindings als HTTPS-->
				<sp:TransportBinding>
					<wsp:Policy>
						<sp:TransportToken>
							<wsp:Policy>
								<sp:HttpsToken />
							</wsp:Policy>
						</sp:TransportToken>
						<sp:AlgorithmSuite>
							<wsp:Policy>
								<sp:Basic128 />
							</wsp:Policy>
						</sp:AlgorithmSuite>
						<sp:Layout>
							<wsp:Policy>
								<sp:Strict />
							</wsp:Policy>
						</sp:Layout>
					</wsp:Policy>
				</sp:TransportBinding>


				<!--				<sp:TransportBinding>
                                    <wsp:Policy>
                                        <sp:TransportToken>
                                            <wsp:Policy>
                                                <sp:HttpsToken RequireClientCertificate="false" />
                                            </wsp:Policy>
                                        </sp:TransportToken>
                                        <sp:AlgorithmSuite>
                                            <wsp:Policy>
                                                <sp:Basic128/>
                                            </wsp:Policy>
                                        </sp:AlgorithmSuite>
                                    </wsp:Policy>
                                </sp:TransportBinding>-->
